<?xml version="1.0"?>
<config>
    <modules>
        <Kega_Soap>
            <version>0.1.0</version>
        </Kega_Soap>
    </modules>
    <global>
		<models>
			<api>
				<rewrite>
					<server_v2_adapter_soap>Kega_Soap_Model_Server_V2_Adapter_Soap</server_v2_adapter_soap>
					<server_v2_handler>Kega_Soap_Model_Server_V2_Handler</server_v2_handler>
				</rewrite>
			</api>
		</models>
    </global>
    <default>
	    <api>
	        <config>
	            <cache>1</cache>
	        </config>
	    </api>
	</default>
</config>